	<subDataset name="Ds_tableVentasCajero" uuid="83f84754-1d63-4ab0-a983-cc5fcb0be6fa">
		<queryString language="SQL">
			<![CDATA[SELECT 
    ROW_NUMBER() OVER (ORDER BY det.Productos_codigo_prod) AS Item,
    prod.nombre_prod as Nombre_Prod, 
    det.Productos_codigo_prod as Codigo_Prod,
    det.cantidad as cantidad,
    prod.valventa_prod as valorUnitario,
    prod.valventa_prod * det.cantidad AS Valor_total
FROM detalle_fac det
JOIN productos prod ON det.Productos_codigo_prod = prod.codigo_prod;]]>
		</queryString>
		<field name="Item" class="java.math.BigInteger"/>
		<field name="Nombre_Prod" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Codigo_Prod" class="java.lang.String"/>
		<field name="cantidad" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="valorUnitario" class="java.math.BigDecimal"/>
		<field name="Valor_total" class="java.math.BigDecimal"/>
	</subDataset>
